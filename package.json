{
  "name": "pti-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-dev": "ng build --watch",
    "build-aot": "ng build --aot",
    "build-owin": "ng build --watch --output-path ../../Pti.WebHost/bin/Debug/wwwroot",
    "build-dev-prod": "ng build --prod",
    "Deployment": "ng build --prod --output-path ../../Pti.WebHost/bin/Deployment/wwwroot --deploy-url",
    "DeploymentCdn": "ng build --prod --output-path ../../Pti.WebHost/bin/Deployment/wwwroot --deploy-url https://ptiglobalqc.azureedge.net/",
    "Deployment2": "ng build --configuration production2 --output-path ../../Pti.WebHost/bin/Deployment2/wwwroot",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ../wwwroot/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.11",
    "@angular/common": "10.0.11",
    "@angular/compiler": "10.0.11",
    "@angular/core": "10.0.11",
    "@angular/forms": "10.0.11",
    "@angular/http": "8.0.0-beta.10",
    "@angular/platform-browser": "10.0.11",
    "@angular/platform-browser-dynamic": "10.0.11",
    "@angular/router": "10.0.11",
    "@ng-select/ng-select": "4.0.0",
    "@types/chart.js": "2.7.46",
    "@types/jquery": "3.3.30",
    "@types/signalr": "2.2.35",
    "bootstrap": "4.5.0",
    "chart.js": "2.7.3",
    "chartjs-plugin-annotation": "0.5.7",
    "core-js": "2.5.4",
    "detect-browser": "4.6.0",
    "file-saver": "2.0.2",
    "font-awesome": "4.7.0",
    "he": "1.2.0",
    "jquery": "3.5.1",
    "ngx-bootstrap": "5.6.0",
    "reduce-store": "1.3.0",
    "rxjs": "6.6.2",
    "signalr": "2.4.1",
    "tslib": "1.10.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.7",
